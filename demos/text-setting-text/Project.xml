<?xml version="1.0" encoding="utf-8"?>
<project>
	<app title="setting-text" file="setting-text" main="Main" version="0.0.1" company="HaxeFlixel" />
	<set name="SWF_VERSION" value="11.8" />
	<window width="320" height="240" fps="30" hardware="false" vsync="false" />
  <window unless="web" background="#000000" />
	<window if="web" orientation="portrait"  />
	<window if="desktop" orientation="landscape" fullscreen="false" resizable="true" />
	<window if="mobile" orientation="landscape" fullscreen="true" width="0" height="0" />
	<set name="BUILD_DIR" value="export" />
	<classpath name="source" />
	<assets path="../demo-assets" />
	<haxelib name="flixel"/>
	<!--haxelib name="flixel-addons" /-->
	<!--haxelib name="flixel-ui" /-->
	<haxedef name="FLX_NO_TOUCH" unless="mobile" />
	<haxedef name="FLX_NO_GAMEPAD" />
	<haxedef name="FLX_NO_FOCUS_LOST_SCREEN" />
	<haxedef name="FLX_NO_DEBUG" unless="debug" />
</project>
